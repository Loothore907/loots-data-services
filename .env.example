# Firebase Configuration
FIREBASE_API_KEY=your-api-key
FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
FIREBASE_APP_ID=your-app-id

# Service Account Path (relative to project root)
FIREBASE_SERVICE_ACCOUNT_PATH=config/service-accounts/service-account.json

# Primary Geocoding Configuration
PRIMARY_GEOCODER_PROVIDER=google
PRIMARY_GEOCODER_API_KEY=your-geocoder-api-key

# Backup Geocoding Configuration
BACKUP_GEOCODER_PROVIDER=rapidapi
BACKUP_GEOCODER_API_KEY=your-rapidapi-key
BACKUP_GEOCODER_API_HOST=forward-reverse-geocoding.p.rapidapi.com