{
  "name": "loots-data-services",
  "version": "0.1.0",
  "description": "Data services for Loot's Ganja Guide",
  "main": "index.js",
  "scripts": {
    "process": "node scripts/process-all.js --input=./data/input/raw_vendors_latest.json --output=./data/output/final_vendors_latest.json",
    "import": "node scripts/import-vendors.js",
    "geocode": "node scripts/geocode-vendors.js --input=./data/processed/norm_vendors_latest.json --output=./data/processed/geo_vendors_latest.json",
    "retry-geocode": "node scripts/geocode-vendors.js --input=./data/failures/geo_failed_latest.json --output=./data/processed/geo_retry_latest.json --provider=rapidapi --delay=1000",
    "finalize": "node scripts/finalize-vendors.js --input=./data/processed/geo_vendors_latest.json --retry=./data/processed/geo_retry_latest.json --output=./data/output/final_vendors_latest.json",
    "sync": "node scripts/sync-firebase.js --input=./data/output/final_vendors_latest.json --mode=overwrite",
    "retry-failed": "node scripts/process-all.js --input=./data/output/failed-vendors.json --output=./data/output/retry-failed-geocoding.json --mode=append",
    "retry-rapidapi": "node scripts/process-all.js --input=./data/output/failed-vendors.json --output=./data/output/rapidapi-retry.json --provider=rapidapi --delay=1000 --mode=append",
    "start": "node server.js",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.1",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "firebase": "^9.15.0",
    "firebase-admin": "^11.4.1",
    "joi": "^17.7.0",
    "multer": "^1.4.5-lts.1",
    "node-geocoder": "^4.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
